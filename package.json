{
  "name": "guess",
  "version": "0.0.0",
  "description": "Smart bundling",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch",
    "test:ci": "jest",
    "postinstall": "ts-node infra/install.ts",
    "build": "ts-node infra/build.ts",
    "publish": "ts-node infra/build.ts --publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guess-js/guess.git"
  },
  "keywords": [
    "bundling",
    "webpack",
    "ml",
    "ai",
    "analytics",
    "recommendation"
  ],
  "author": "Minko Gechev <mgechev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guess-js/guess/issues"
  },
  "homepage": "https://github.com/guess-js/guess#readme",
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/meow": "^4.0.1",
    "@types/node": "^9.4.6",
    "jest": "^22.4.2",
    "meow": "^4.0.0",
    "prompt-confirm": "^1.2.0",
    "raw-loader": "^0.5.1",
    "ts-jest": "^22.4.1",
    "ts-loader": "^3.5.0",
    "ts-node": "^6.0.2",
    "tslint": "^5.9.1",
    "typescript": "^2.9.0",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.0.11"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "<rootDir>/packages/detector/__tests__/fixtures",
      "<rootDir>/packages/parser/__tests__/fixtures"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
